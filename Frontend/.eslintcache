.gridList} cellHeight={300} >\n    <Card>\n        <CardContent>\n    <ComposableMap\n    projectionConfig={{\n        scale: 155,\n        rotation: [-11, 0, 0],\n      }}\n      width={800}\n      height={400}\n      padding={10}\n      style={{ width: \"100%\", height: \"auto\" }}\n    >\n      <Graticule stroke=\"#DDD\" />\n      <Geographies\n        geography={geoUrl}\n        fill=\"#D6D6DA\"\n        stroke=\"#FFFFFF\"\n        strokeWidth={0.5}\n      >\n        {({ geographies }) =>\n          geographies.map(geo => <Geography key={geo.rsmKey} geography={geo} />)\n        }\n      </Geographies> \n      <DrawLine value1 = {count1} indices = {wrongAnsIndices}/>\n            {markers.map(({ name, coordinates, markerOffset }) => (\n        <Marker key={name} coordinates={coordinates}>\n          <circle r={5} fill=\"#F00\" stroke=\"#fff\" strokeWidth={2} />\n          <text\n            textAnchor=\"middle\"\n            y={markerOffset}\n            style={{ fontFamily: \"system-ui\", fill: \"#5D5A6D\" }}\n          >\n            {name}\n          </text>\n        </Marker>\n      ))}\n    </ComposableMap>\n    </CardContent>\n    </Card>\n    <Card>\n    <CardContent>\n      <p>{DashData[count1].question}</p>\n    <CardActions>\n      <IconButton onClick={CorrectClick} aria-label = \"Correct\">\n          <CheckIcon className={classes.icon} />\n      </IconButton>\n      <IconButton onClick={() => setWrongAnsIndices(wrongAnsIndices=> [...wrongAnsIndices, count1])} aria-label = \"Incorrect\">\n          <ClearIcon className={classes.icon} />\n      </IconButton>\n    </CardActions>\n    </CardContent>\n    </Card>\n    </GridList>\n  );\n};\n\nexport default MapChart;\n","/Users/Mahendra/Desktop/Capstone/SER517/Frontend/src/components/Student.js",[],"/Users/Mahendra/Desktop/Capstone/SER517/Frontend/src/firebaseConfig.js",[],"/Users/Mahendra/Desktop/Capstone/SER517/Frontend/src/components/DashData.js",[],{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","repl